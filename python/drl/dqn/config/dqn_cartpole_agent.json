{
  "loglevel": "info",
  "memory_capacity": 1e5,
  "batch_size": 32,
  "update_rate": 0.25,
  "update_repeat": 1,
  "use_target_network": true,
  "target_network_update_rate": 0.0001,
  "min_replay_size": 5e4,
  "learning_rate": 0.00025,
  "optimizer": "tensorflow.python.training.rmsprop.RMSPropOptimizer",
  "double_dqn": false,
  "optimizer_param": {
    "momentum": 0.95,
    "epsilon": 0.01
  },
  "exploration": "epsilon_decay",
  "exploration_param": {
    "epsilon": 1,
    "epsilon_final": 0.1,
    "epsilon_states": 1e6
  },

  "repeat_actions": 1
}
